crds:
  create: false
customResourceManagerDisabled: true
env:
  AWS_REGION: eu-west-1
  AWS_DEFAULT_REGION: eu-west-1
  POLLER_INTERVAL_MILLISECONDS: 10000
  LOG_LEVEL: info
  LOG_MESSAGE_KEY: msg
  METRICS_PORT: 3001
  USE_HUMAN_READABLE_LOG_LEVELS: true
image:
  repository: godaddy/kubernetes-external-secrets
  tag: 5.2.0
  pullPolicy: IfNotPresent
rbac:
  create: true
replicaCount: 1
resources:
  limits:
    cpu: 100m
    memory: 128Mi
  requests:
    cpu: 100m
    memory: 128Mi
securityContext:
  runAsNonRoot: true
  fsGroup: 65534
serviceAccount:
  create: false
  name: external-secrets
serviceMonitor:
  enabled: false
  interval: 30s
  namespace: ""
