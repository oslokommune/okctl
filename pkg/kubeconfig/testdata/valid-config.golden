apiVersion: v1
clusters:
- cluster:
    certificate-authority-data: c29tZXRoaW5n
    server: https://something
  name: okctl-pro.eu-west-1.eksctl.io
contexts:
- context:
    cluster: okctl-pro.eu-west-1.eksctl.io
    user: someuser@okctl-pro.eu-west-1.eksctl.io
  name: someuser@okctl-pro.eu-west-1.eksctl.io
current-context: someuser@okctl-pro.eu-west-1.eksctl.io
kind: Config
preferences: {}
users:
- name: someuser@okctl-pro.eu-west-1.eksctl.io
  user:
    exec:
      apiVersion: client.authentication.k8s.io/v1alpha1
      args:
      - token
      - -i
      - okctl-pro
      command: aws-iam-authenticator
      env:
      - name: AWS_STS_REGIONAL_ENDPOINTS
        value: regional
      - name: AWS_DEFAULT_REGION
        value: eu-west-1
      - name: AWS_PROFILE
        value: default
      provideClusterInfo: false
