apiVersion: argoproj.io/v1alpha1
kind: Application
metadata:
  name: okctl-config
  namespace: argocd
spec:
  destination:
    namespace: argocd
    server: https://kubernetes.default.svc
  project: default
  source:
    path: infrastructure/mock-cluster/okctl
    repoURL: git@github.com:oslokommune/mock-repo
    targetRevision: HEAD
  syncPolicy:
    automated:
      prune: false
      selfHeal: false

---

