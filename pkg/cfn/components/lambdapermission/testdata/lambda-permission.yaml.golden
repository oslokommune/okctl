AWSTemplateFormatVersion: 2010-09-09
Resources:
  myPermission:
    Properties:
      Action: lambda:InvokeFunction
      FunctionName:
        Fn::GetAtt:
        - myFunction
        - Arn
      Principal: secretsmanager.amazonaws.com
      SourceAccount:
        Ref: AWS::AccountId
    Type: AWS::Lambda::Permission
