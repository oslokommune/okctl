apiVersion: okctl.io/v1alpha1
clusterRootDomain: okctl-stage.oslo.systems
experimental:
  automatizeZoneDelegation: false
github:
  organisation: oslokommune
  outputPath: infrastructure
  repository: okctl-iac
integrations:
  argoCD: true
  autoscaler: true
  awsLoadBalancerController: true
  blockstorage: true
  cognito: true
  externalDNS: true
  externalSecrets: true
  kubePromStack: true
  loki: true
  promtail: true
  tempo: true
kind: Cluster
metadata:
  accountID: "123456789012"
  name: okctl-stage
  region: eu-west-1
vpc:
  cidr: 192.168.0.0/20
  highAvailability: true
