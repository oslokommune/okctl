AWSTemplateFormatVersion: 2010-09-09
Outputs:
  myResource:
    Export:
      Name:
        Fn::Sub: ${AWS::StackName}-myResource
    Value:
      Ref: myResource
Resources:
  myResource:
    Properties:
      AccessControl: BucketOwnerFullControl
      BucketName: my-Bucket
      PublicAccessBlockConfiguration: {}
    Type: AWS::S3::Bucket
