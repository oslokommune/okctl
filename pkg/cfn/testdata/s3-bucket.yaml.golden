AWSTemplateFormatVersion: 2010-09-09
Outputs:
  BucketARN:
    Export:
      Name:
        Fn::Sub: ${AWS::StackName}-BucketARN
    Value:
      Fn::GetAtt:
      - S3Bucket
      - Arn
  S3Bucket:
    Export:
      Name:
        Fn::Sub: ${AWS::StackName}-S3Bucket
    Value:
      Ref: S3Bucket
Resources:
  S3Bucket:
    Properties:
      AccessControl: BucketOwnerFullControl
      BucketName: myBucket
    Type: AWS::S3::Bucket
