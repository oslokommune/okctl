AWSTemplateFormatVersion: 2010-09-09
Outputs:
  S3Bucket:
    Export:
      Name:
        Fn::Sub: ${AWS::StackName}-S3Bucket
    Value:
      Ref: S3Bucket
Resources:
  S3Bucket:
    Properties:
      AccessControl: BucketOwnerFullControl
      BucketName: myBucket
      PublicAccessBlockConfiguration: {}
    Type: AWS::S3::Bucket
