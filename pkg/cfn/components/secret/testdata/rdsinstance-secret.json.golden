AWSTemplateFormatVersion: 2010-09-09
Outputs:
  myAdminSecret:
    Export:
      Name:
        Fn::Sub: ${AWS::StackName}-myAdminSecret
    Value:
      Ref: myAdminSecret
Resources:
  myAdminSecret:
    Properties:
      GenerateSecretString:
        ExcludeCharacters: '"@/\'
        GenerateStringKey: password
        PasswordLength: 16
        SecretStringTemplate: '{"username": "admin"}'
      Name: myAdminSecret
    Type: AWS::SecretsManager::Secret
