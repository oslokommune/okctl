AWSTemplateFormatVersion: 2010-09-09
Outputs:
  myBucketrepoenvS3Bucket:
    Export:
      Name:
        Fn::Sub: ${AWS::StackName}-myBucketrepoenvS3Bucket
    Value:
      Ref: myBucketrepoenvS3Bucket
Resources:
  myBucketrepoenvS3Bucket:
    Properties:
      AccessControl: BucketOwnerFullControl
      BucketName: myBucket
    Type: AWS::S3::Bucket
